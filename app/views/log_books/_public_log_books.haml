%h1
  Public Log Books
%br
.row-fluid
  .span12
    %table.table.table-condensed
      %thead
        %th='Title'
        %th='Game'
        %th='No.&nbsp;of&nbsp;Items'.html_safe
        %th
      -public_log_books.each do |log_book|
        %tr
          %td=link_to log_book.title.html_safe, log_book
          %td=log_book.game_name.html_safe
          %td=log_book.world_objects.count
          %td=log_book_access_indicator_icon(log_book)
